{
  "name": "ai-test",
  "version": "1.0.0",
  "description": "Setup for AI tool test",
  "main": "index.tsx",
  "scripts": {
    "analyze": "webpack-bundle-analyzer stats.json ",
    "build:analyze": "webpack --config webpack.analyze.js",
    "build:static": "webpack --config webpack.static.js && cp _redirects ./build-static && serve ./build-static",
    "build-storybook": "build-storybook",
    "build": "webpack --config webpack.prod.js && scripts/componentsMap.sh",
    "cy:open:local": "./scripts/test-run.sh cypress open",
    "cy:refresh-features": "cp -rf cypress/integration && node ./scripts/remove-old-features.js",
    "cy:test": "cypress run --env TAGS=\"@e2e-test\" --spec \"cypress/integration/**/*.feature\"",
    "cy:test:local": "./scripts/test-run.sh cy:test",
    "cy:test:video": "npm run cy:test -- --config video=true",
    "cypress:test": "wait-on http://localhost:6452 && cypress run",
    "cypress:wait:start": "wait-on http://localhost:9090 && cross-env CYPRESS_ENVIRON=localhost-e2e cypress run -c integrationFolder=cypress/e2e",
    "format": "npm run lint -- --fix && prettier --write src/**/*.{js,ts,tsx}",
    "lint": "eslint  --config ./.eslintrc \"{src,cypress,scripts}/**/*.{js,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint-staged": "lint-staged --config ./.eslintrc",
    "prepare": "husky install",
    "report:combined": "nyc report --reporter lcov --report-dir coverage",
    "start": "webpack serve --open --config webpack.dev.js",
    "start:e2e": "webpack serve --config webpack.cypress.js",
    "storybook": "./scripts/test-run.sh start-storybook -p 6452",
    "test:coverage": "npm run test:jest -- --coverage --colors",
    "test:cypress:e2e": "concurrently \"npm run mock-server:cypress\" \"npm run start:e2e\" \"npm run cypress:wait:start\" -k -s first",
    "test:cypress": "concurrently \"npm run storybook -- --ci\" \"npm run cypress:test\" -k -s first",
    "test:cypress:local": "rm -rf .nyc_output cypress-coverage && npm run test:cypress && open ./cypress-coverage/index.html",
    "test:generate-output": "npm run test:jest -- --runInBand --json --outputFile=.jest-test-results.json",
    "test:jest": "npm run typecheck && cross-env NODE_ICU_DATA=node_modules/full-icu jest --config ./configs/jest.config.js",
    "test": "npm run test:jest && npm run test:cypress",
    "test-local": "npm test && cypress run",
    "test:watch": "npm run test --watch",
    "typecheck": "tsc --noEmit"
  },
  "author": "Ustop",
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.6",
    "react-responsive": "^8.2.0",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1",
    "regenerator-runtime": "^0.13.11",
    "ssr-window": "4.0.2",
    "styled-components": "5.3.5",
    "styled-normalize": "^8.0.7",
    "uuid": "^8.3.2",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@cfaester/enzyme-adapter-react-18": "0.7.0",
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/webpack-preprocessor": "^5.17.1",
    "@storybook/addon-a11y": "6.5.9",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-controls": "6.5.9",
    "@storybook/addon-docs": "6.5.9",
    "@storybook/addon-jest": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/addon-notes": "5.3.21",
    "@storybook/addon-storysource": "6.5.9",
    "@storybook/addon-viewport": "6.5.9",
    "@storybook/addons": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/react": "6.5.9",
    "@storybook/theming": "6.5.9",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/enzyme": "3.10.11",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/styled-components": "5.1.24",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-styled-components": "^2.1.1",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "^9.2.0",
    "concurrently": "7.0.0",
    "copy-webpack-plugin": "10.2.4",
    "cpx": "^1.5.0",
    "cross-env": "7.0.3",
    "css-loader": "^6.7.3",
    "cypress": "^9.7.0",
    "cypress-axe": "^0.14.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "fetch-mock": "^9.11.0",
    "file-loader": "^6.2.0",
    "fs-extra": "11.1.1",
    "full-icu": "1.4.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-lib-coverage": "3.2.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "28.1.3",
    "jest-fetch-mock": "3.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "json-server": "^0.17.3",
    "node-fetch": "^3.3.1",
    "nyc": "^15.1.0",
    "prettier": "2.8.4",
    "react-docgen-typescript-loader": "3.7.2",
    "sass": "1.54.0",
    "sass-loader": "13.0.2",
    "serve": "^13.0.4",
    "style-loader": "^3.3.2",
    "svgo": "2.8.0",
    "terser-webpack-plugin": "5.3.1",
    "to-string-loader": "^1.2.0",
    "ts-jest": "29.0.5",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "wait-on": "^6.0.1",
    "webpack": "5.70.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "^4.13.3",
    "webpack-manifest-plugin": "5.0.0",
    "webpack-merge": "5.8.0"
  },
  "testCoverage": {
    "branchesToChapter": [
      "master"
    ],
    "jest": 80,
    "cypress": 50
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "nyc": {
    "report-dir": "cypress-coverage",
    "reporter": [
      "json",
      "json-summary",
      "html"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*.test.js",
      "**/test.js",
      "**/*.test.ts",
      "**/*.test.tsx",
      "**/*.stories.js",
      "**/*.stories.tsx",
      "**/stories.js",
      "src/setupTests.js"
    ]
  },
  "cli": {
    "sonarExclusions": [
      "**/*.stories.js",
      "**/*.stories.tsx",
      "cypress/**/*.*",
      "scripts/*.*",
      "**/__mocks__/*.js",
      "src/index.tsx",
      "src/*.js",
      "**/__tests__/*.*",
      "src/Imports/*.js",
      "src/componentsMap.tsx"
    ]
  }
}
